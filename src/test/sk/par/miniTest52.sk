
struct Box{
	int val;	
}

bit test(int in){	
	return 1;
}


static int foo(Box g){
	int t = 0;
	reorder{
		return t;
		t = g.val;
		t = t + ??;	
	}
}

bit testSK(int in) implements test{
	Box g = new Box();
	g.val = in + ??;
	fork(int i; 2){
		g.val = foo(g);	
	}
	assert g.val == in + 4 || g.val == in + 3;
	return 1;
}




