void setAdd(Set s, int key){
	Node n = newNode(key);
	bit outMore = 1;
	for(int i=0; i<3; ++i){
		if(outMore){
			Node cur = s.head;
			Node pred = null;
			bit more = 1;
			for(int j=0; j<3; ++j){
				if(more && cur.val < key){
					pred = cur;	
					cur = cur.next;
				}else{
					more = 0;	
				}
			}
			Node lk = null;
			lk = pred.next;
			lock(lk);
			atomic{
			if(pred.next == cur){
				 n.next = cur;
				 pred.next = n;
				 outMore = 0;
			}
			}
			unlock(lk);
		}
	}
}


