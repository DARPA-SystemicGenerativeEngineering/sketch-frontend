struct Node{
	int val;
	Node next;
}

Node newNode(int i){
	Node n = null;
	atomic{ 
		n = new Node();
	}
	n.val = i;
	n.next = null;	
	return n;
}

struct Set{
	Node head;	
}

Set newSet(){
	Set s = null;
	atomic{
		s = new Set();
	}
	s.head = null;
	return s;	
}


void setInit(Set s){
	int key1 = 0;
	int key2 = 9;
	Node n1 = newNode(key1);
	Node n2 = newNode(key2);
	n1.next = n2;
	s.head = n1;
}



