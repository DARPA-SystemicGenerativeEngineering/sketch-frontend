}

Node someNext(Node n){
	if(??){
		return n;	
	}else{
		Node t= someNext(n);
		return t.next;	
	}
	return null;
}

Node gen(Queue q){
	Node n = baseQ(q);
	return someNext(n);	
}

void enqueue(Queue q, int i){
	Node n = null;
	atomic{
		n = new Node();
	}
	n.val = i;
	n.next = null;
	Node ot = null;
	anyorder{
		ot.next = n;
		atomic{
			ot = gen(q);
			q.tail = n; 
		}		
	}
}

int dequeue(Queue q){
	Node n = q.prevHead;
	if(n.next != null){
		q.prevHead = n.next;
		return q.prevHead.val;	
	}else{
		return -1;	
	}
}

