struct Node{
	int val;
	Node next;
}

struct Queue{
	Node prevHead;
	Node tail;	
}


Queue newQueue(){
	Queue q = new Queue();
	q.prevHead = new Node();
	q.prevHead.next = null;
	q.tail = q.prevHead;	
	return q;
}

Node baseQ(Queue q){
	if(??){
