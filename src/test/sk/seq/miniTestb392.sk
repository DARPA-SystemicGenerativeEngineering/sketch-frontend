struct Foo{
    int x;
    int y;
}

struct Bar{
    Foo f;
    Bar b;
}

harness void main(int a, int b, int c, int d, int e, int g){
    Foo f = new Foo(x=a, y=b);
    assert f.x == a && f.y == b;
    
    Bar b = new Bar(f=new Foo(x=c, y=d), b=new Bar(f=new Foo(x=e, y=g)));
    assert b.f.x == c && b.f.y == d && b.b.f.x==e && b.b.f.y==g;
    
    Bar b = new Bar(f=new Foo(x=((a!=10)&&(e/(a-10))>0)? a :e ));
    assert b.b == null;
    assert b.f.x != 10 || e == 10;
    
    
}