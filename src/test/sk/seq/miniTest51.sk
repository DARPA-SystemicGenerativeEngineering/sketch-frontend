bit reverseSketch(bit[3] in, bit[3] in2) implements reverse {
	int [8] fu;
	for(int i=0; i<8; ++i){
		fu[i] = {*};
	}
	int idx1 = (int) in;
	int idx = fu[ idx1 ];
	return in2[idx];
}


bit reverse (bit[3] in, bit[3] in2) {
	int idx1 = (int) in;
	int idx = idx1;
	 if( idx1 > 2){
	 	idx = 2;
	 }
	return in2[idx];
}