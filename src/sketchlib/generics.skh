

T[n] map<T>([int n], T[n] in, fun f){
	T[n] out;
	for(int i=0; i<n; ++i){
		out[i] = f(in[i]);
	}
} 

int len<T>([int n], T[n] ar){
	return n;
} 

void foreach<T>([int n], ref T[n] in, fun f){
	for(int i=0; i<n; ++i){
		f(in[i]);
	}
}

void forall(int a, int b, fun f){
	for(int i=a; i<b; ++i){
		f(i);
	}
}
