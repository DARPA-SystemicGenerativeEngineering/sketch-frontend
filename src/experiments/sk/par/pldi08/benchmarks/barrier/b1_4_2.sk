

int N = 4;
int T = 2;

struct barrier{
	int count;	
	bit current;
	bit[N] bits;
}

generator barrier newBarrier(){ /* automatically rewritten */
	barrier b = new barrier();	
	b.count = N;
	b.bits = 0;	
	b.current = ??;
	return b;
}


generator bit predicate(int a, int b, bit c){ /* automatically rewritten */
	bit rv = 0;
	if(??){
		rv = c;
	}else{
		if(??){
			rv = a == b;	
		}else{
			rv = ( ?? ? a : b) == ??;
		}
	}
	if(??){
		rv = !rv;	
	}
	return rv;
}

void next(barrier b, int thread){ /* automatically rewritten */
	bit s = b.bits[thread];
	s = predicate(0,0,s);
	int cv = 0;
	
	reorder{
		b.bits[thread] = s;	
	
		atomic{
			cv = b.count;
			b.count = b.count - 1;	
		}
		
		if(predicate(b.count,cv,s)){
			reorder{
				b.count = N;
				b.current = predicate(b.count,cv,s);
			}
		}else{
			bit t = predicate(0,0,s);
			atomic(b.current == t){
			}	
		}
	}
}
bit test(){
	return 1;	
}


generator int left(int t){ /* automatically rewritten */
	int rv = 0;
	rv = t - 1;
	if( rv < 0){ rv = rv + N; }
	return rv;	
}

generator int right(int t){ /* automatically rewritten */
	int rv = 0;
	rv = t + 1;
	if( rv >= N){ rv = rv - N; }
	return rv;	
}

bit sketch() implements test{
	
	bit[N][T] grid = 0;
	barrier b = newBarrier();
	fork(int thread; N){
		for(int i=0; i<T; ++i){
			if(i > 0){
				assert grid[i-1][left(thread)] /*&& grid[i-1][right(thread)]*/; 	
			}		
			grid[i][thread] = 1;
			next(b, thread);
		}		
	}
	return 1;
}
