int max_len = 6;
int len = 6;

struct Array {
    int [max_len] A;
    int len;
}


int[len+1] append(Array ar, int b) {
  int[len+1] res = 0;
  for(int i=0; i<len;i++)
    res[i] = ar.A[i];
  res[len+1] = b;
  return res;
}

Array init(Array ar) {
  ar.A = {1,2,3,4,5,6};
  return ar;
}

harness void appendCheck() {
  Array arr = init(new Array());
  int b = 7;
  int[7] t = append(arr, b);
  assert t == {1,2,3,4,5,6,7};
}