pragma options "--bnd-inbits 5 --bnd-inline-amnt 32";

int multSpec(int n, int m) {
     if (m == 0) {
              return 0;
      } else {
               return n + multSpec(n, m - 1);
      }
}

int mult(int n, int m) implements multSpec{
      if (m == 0) {
              return ??;
      } else {
              return n + mult(n, m - ??);
      }
}