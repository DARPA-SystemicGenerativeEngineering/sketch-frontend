generator int[N] set(bit[N][N] edges, int x) {
	return {| s(edges, x) | p(edges, x) | sPlus(edges, x) | pPlus(edges, x) | sStar(edges, x) | pStar(edges, x) |};
}

generator bit cond(bit[N][N] edges, int x) {
	return {| ?? | (countS(edges, x) | countP(edges, x) | countSPlus(edges, x) | countPPlus(edges, x) | countSStar(edges, x) | countPStar(edges, x)) (!= | >) (0|1) |};	
}

bit CLLspec(bit[N][N] edges) {
	/*root r via N*/
	int[N] viaR = sStar(edges, 0);
	for(int i = 0; i < N; i++) {
		if(!isPresent(viaR, i))
			return false;
	}
	/*Functional N*/
	for(int i = 0; i < N; i++) {
			int sum = 0;
			for(int j = 0; j < N; j++) {
				sum += (int)edges[i][j];
			}
			if(sum > 1) {
				return false;
			}
	}
	/*No self-loops*/
	for(int i = 0; i < N; i++) {
			if(isPresent(s(edges, i), i))
				return false;
	}
	/*\forall u, v (N^*(u, v))*/
	for(int i = 0; i < N; i++) {
		for(int j = 0; j < N; j++) {
			if(!isPresent(sStar(edges, i), j))
				return false;
		}
	}
	return true;
}

bit CLLsketch(bit[N][N] edges) implements CLLspec {
	/*root r via N*/
	int[N] viaR = sStar(edges, 0);
	for(int i = 0; i < N; i++) {
		if(!isPresent(viaR, i))
			return false;
	}
	/*Functional N*/
	for(int i = 0; i < N; i++) {
			int sum = 0;
			for(int j = 0; j < N; j++) {
				sum += (int)edges[i][j];
			}
			if(sum > 1) {
				return false;
			}
	}
	/*No self-loops*/
	for(int i = 0; i < N; i++) {
			if(isPresent(s(edges, i), i))
				return false;
	}
	minrepeat {
		bit condP = {| ?? | (countS(edges, 0) | countP(edges, 0) | countSPlus(edges, 0) |  countPPlus(edges, 0) |countSStar(edges, 0) | countPStar(edges, 0)) (!= | >) (0|1) |};	
		if(condP)
			return false;
		for(int i = 1; i < N; i++) {
			bit condO = {| ?? | (countS(edges, i) | countP(edges, i) | countSPlus(edges, i) |  countPPlus(edges, i) |countSStar(edges, i) | countPStar(edges, i)) (!= | >) (0|1) |};	
			if(condO)
				return false;
		}
	}
	return true;
}
