#define N 3

pragma options "--debug-verbosity 4 --bnd-cost-estimate 4 --bnd-cost-time-multiple 10 --slv-timeout 600";

int[N] s(bit[N][N] rels, int x) {
	int[N] result;
	for(int i = 0; i < N; i++) {
		result[i] = -1;
	}
	int count = 0;
	for(int i = 0; i < N; i++) {
		if(rels[x][i]) {
			result[count] = i;
			count++;
		}
	}
	return result;
}

int countS(bit[N][N] rels, int x) {
	int[N] result = s(rels, x);
	int count = 0;
	for(int i = 0; i < N; i++) {
		if(result[i] == -1)
			return count;
		count++;
	}
	return count;
}

bit isPresent(int[N] arr, int x) {
	for(int i = 0; i < N; i++) {
		if(arr[i] == x)
			return true;
	}
	return false;
}

int[N] merge(int[N] arr1, int[N] arr2) {
	int[N] result; 
	int count = 0;
	for(int i = 0; i < N; i++) {
		if(arr1[i] != -1) {
			result[count] = arr1[i];
			count++;
		}
	}
	for(int i = 0; i < N; i++) {
		if(arr2[i] != -1 && !isPresent(result, arr2[i])) {
			result[count] = arr2[i];
			count++;
		}
	}
	for(int i = count; i < N; i++) {
		result[i] = -1;
	}
	return result;
}

int[N] sPlus(bit[N][N] rels, int x) {
	int[N] result;
	int[N] curr;
	int[N] processed;
	for(int i = 0; i < N; i++) {
		result[i] = -1;
		curr[i] = -1;
		processed[i] = -1;
	}
	curr[0] = x;
	int count = 0;
	int processedCount = 0;
	while(count != -1) {
		int[N] temp;
		temp = s(rels, curr[0]);
		processed[processedCount] = curr[0];
		processedCount++;
		for(int i = 0; i < N - 1; i++) {
			curr[i] = curr[i + 1];
		}
		count--;	
		if(temp[0] != -1) {
			result = merge(result, temp);
			for(int i = 0; i < N; i++) {
				if(temp[i] != -1 && !isPresent(processed, temp[i]) && !isPresent(curr, temp[i])) {
					count++;
					curr[count] = temp[i];
				}				
			}
		}
	}
	return result;
}

int countSPlus(bit[N][N] rels, int x) {
