bit[32] reverseSketch(bit[32] in) implements reverse {

	bit[32]  t = in;	
	int s = 1;
	loop({*}){
		bit[32] tmp1 = (t >> s);
		bit[32] tmp2 = (t << s);
		t = tmp1 {|} tmp2;
		s = s*2;
		
	}

	return t;
}


bit[32] reverse (bit[32] in) {
	bit [32] out;
	for (int i = 0; i < 32; i++) {
		out[i] = in[31 - i];
	}
	return out;
}
