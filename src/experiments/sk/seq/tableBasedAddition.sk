int W = 4;

bit[W] reverseSketch(bit[W] in1, bit[W] in2) implements reverse {
	int exp = 1<<(W*2);
	bit [W*exp] tmp={*};
	bit[W] output;
	bit[2*W] input;
	for(int i=0; i<W; ++i){
		input[i] = in1[i];
		input[i+W] = in2[i];
	}
	int idx = (int) input;
	for(int i=0; i<W; ++i){
		output[i] = tmp[idx*W+i];
	}
	return output;
}

bit[W] reverse (bit[W] in1, bit[W] in2) {	
	return in1 + in2;
}