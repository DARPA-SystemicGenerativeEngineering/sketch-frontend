int W = 32;
bit [W] mult(bit[W] in1, bit[W] in2){
	bit [W] result = 0;
	for(int i=0; i<W; ++i){
		if(in2[i]==1){
			result = result + in1;
		}
		in1 = in1 << 1;
	}
	return result;
}

bit[W] reverseSketch(bit[W] in) implements reverse {
	bit [W] tmp;
	tmp = {*};
	return 	mult(in , tmp);
}

bit[W] reverse (bit[W] in) {
	bit [W] tmp = 0;
	loop(7){
		tmp = tmp + in;
	}
	return tmp;
}