int N;
int[N] test(int[N] in){
  int[N] out;
  for(int i=0; i<N; ++i){
  	if(i>0 && i<N-1) out[i] = in[i-1] + in[i+1];
  	else out[i] = 0;
  }
  return out;
}


int[N] testSK(int[N] in) implements test {
  int[N] out = 0;
  
  
  for(int i=2; i<N-1; ++i){  	
  	out[i] = in[i-1] + in[i+1];
  }
  return out;
}