int N;
int[N] test(int[N] in){
  int[N] out;
  for(int i=0; i<N; ++i){
  	if(i>0 && i<N-1) out[i] = in[i-1] + in[i+1];
  	else out[i] = 0;
  }
  return out;
}


int[N] testSK(int[N] in) implements test{
  int[N] out;
  int t0 = in[0];
  int t1 = in[1];
  out[0] = 0;
  out[N-1] = 0;
  for(int i=1; i<N-1; ++i){
    int t2 = in[i+1]; 
  	out[i] = t0 + t2;
  	t0 = t1;
  	t1 = t2;
  }
  return out;
}