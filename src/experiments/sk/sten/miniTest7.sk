int N;
int[N][N] test(int[N][N] in){
  int[N][N] out;
  for(int i=0; i<N; ++i){
    for(int j=0; j<N; ++j){
    	if(i>0 && i<N-1 && j>0 && j<N-1) out[i][j] = in[i-1][j] + in[i+1][j] + in[i][ j-1] + in[i][j+1];
  		else out[i][j] = 0;
    }
  }
  return out;
}


