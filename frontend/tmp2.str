
void->void pipeline Minimal {
	add IntSource();
	add DropThird();
	add IntPrinter();
}

void->int filter IntSource {
	int x;
	init {
		x = 1; 
	}
	work push 1 {
		push(x);
		x = x + 1;
	}
}

int->int filter DropThird{
	init{}
	work push 1 pop 1{
	push(pop());
	}
}

int->void filter IntPrinter {
	init {
	}
	work pop 1 {
	print(pop());
	}
}



